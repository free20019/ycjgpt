          <!-- <div class=\"tw-title\">综合占比：</div> -->\r\n            <div class=\"tw-title\">全部：{{ leftTopData.all }}</div>\r\n            <div class=\"tw-title\">滴滴：{{ leftTopData.dd }}</div>\r\n            <div class=\"tw-echart\" id=\"rightEchart\"></div>\r\n          </div>\r\n          <div class=\"tw-echart-right\">\r\n            <div class=\"tw-echart\" id=\"rightRightEchart\"></div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"tw-layout-foot\">\r\n      <div class=\"tw-tab clearfix\">\r\n        <div\r\n          class=\"tw-tab-item\"\r\n          :class=\"[tab.avg == '行业日均' ? ' active' : 'no-active']\"\r\n          @click=\"handleTabAvg('行业日均')\"\r\n        >\r\n          行业日均\r\n        </div>\r\n        <div\r\n          class=\"tw-tab-item\"\r\n          :class=\"[tab.avg == '单车日均' ? ' active' : 'no-active']\"\r\n          @click=\"handleTabAvg('单车日均')\"\r\n        >\r\n          单车日均\r\n        </div>\r\n      </div>\r\n      <div class=\"tw-echart-area danche\" v-show=\"tab.avg == '单车日均'\">\r\n        <div class=\"tw-item\" @click=\"echartClick\">\r\n          <div class=\"tw-title\">企业单车日均订单量</div>\r\n          <div class=\"tw-echart\" id=\"daySend\"></div>\r\n        </div>\r\n        <div class=\"tw-item\">\r\n          <div class=\"tw-title\">企业单车日均收益(元)</div>\r\n          <div class=\"tw-echart\" id=\"dayGet\"></div>\r\n        </div>\r\n        <div class=\"tw-item\">\r\n          <div class=\"tw-title\">企业单车日均载客里程(公里)</div>\r\n          <div class=\"tw-echart\" id=\"revenue\"></div>\r\n        </div>\r\n        <div class=\"tw-item\">\r\n          <div class=\"tw-title\">企业单车日均载客时长(时)</div>\r\n          <div class=\"tw-echart\" id=\"carRate\"></div>\r\n        </div>\r\n      </div>\r\n      <div class=\"tw-echart-area hangye\">\r\n        <div class=\"tw-item\">\r\n          <div class=\"tw-title\">平台日均经营情况</div>\r\n          <div class=\"tw-echart\" id=\"jingyingqingkuang\"></div>\r\n        </div>\r\n        <div class=\"tw-item\">\r\n          <div class=\"tw-title\">平台日均接单时长</div>\r\n          <div class=\"tw-echart\" id=\"jiedanshichang\"></div>\r\n        </div>\r\n        <div class=\"tw-item\">\r\n          <div class=\"tw-title\">平台日均活跃车辆</div>\r\n          <div class=\"tw-echart\" id=\"huoyuecheliang\"></div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport echarts from 'echarts'\r\nimport { loading } from '../../assets/js/util'\r\nimport * as a from '../../assets/js/api.js'\r\nimport _ from 'underscore'\r\nexport default {\r\n  name: 'OperationOverview',\r\n  data() {\r\n    return {\r\n      echart: null,\r\n      tab: {\r\n        date: '今日',\r\n        avg: '行业日均'\r\n      },\r\n      avgEchart: {\r\n        daySend: null,\r\n        dayGet: null,\r\n        revenue: null,\r\n        carRate: null\r\n      },\r\n      hangyeEchart: {\r\n        jingyingqingkuang: null,\r\n        jiedanshichang: null,\r\n        huoyuecheliang: null\r\n      },\r\n      rightEchart: {\r\n        right: null,\r\n        left: null\r\n      },\r\n      mapEchart: {\r\n        onlineTotal: null,\r\n        szlTotal: null\r\n      },\r\n      table: {\r\n        data: []\r\n      },\r\n      permitNum: {\r\n        comp: 0,\r\n        driver: 0,\r\n        vehi: 0\r\n      },\r\n      dayData: {\r\n        jrpd: 0,\r\n        jrwd: 0,\r\n        jrsc: 0,\r\n        jrhy: 0,\r\n        zrpd: 0,\r\n        zrwd: 0,\r\n        zrsc: 0,\r\n        zrhy: 0,\r\n        szpd: 0,\r\n        szwd: 0,\r\n        szsc: 0,\r\n        szhy: 0,\r\n        sypd: 0,\r\n        sywd: 0,\r\n        sysc: 0,\r\n        syhy: 0\r\n      },\r\n      current: [],\r\n      dayList: [],\r\n      tck: {\r\n        hycl: 0,\r\n        ddxq: 0,\r\n        ddcj: 0,\r\n        areaName: ''\r\n      },\r\n      dqzxzl: 0,\r\n      leftTopData: {\r\n        all: 0,\r\n        dd: 0,\r\n        loading: false,\r\n        activeName: 'huoyuecheliang',\r\n        data: []\r\n      },\r\n      warning: {}\r\n    }\r\n  },\r\n  mounted() {\r\n    this.$nextTick(() => {\r\n      // let load = loading()\r\n      // setTimeout(() => {\r\n      //   load.close()\r\n      // }, 1000)\r\n      setTimeout(() => {\r\n        this.querypermitNum()\r\n        this.getDayData()\r\n        this.iDayAver()\r\n        this.getAreaSituation()\r\n        this.getService()\r\n        // this.initEchart()\r\n        // this.initOnlineTotalEchart()\r\n        // this.initSzlTotalEchart()\r\n        this.initRightEchart()\r\n        this.initRightLeftEchart()\r\n        this.initJYqingkuang()\r\n        this.getEarlyWarningNum()\r\n        // this.initjiedanshichang()\r\n        // this.inithuoyuecheliang()\r\n      }, 500)\r\n\r\n      window.onresize = () => {\r\n        setTimeout(() => {\r\n          // this.echart.resize()\r\n          this.rightEchart.left.resize()\r\n          this.rightEchart.right.resize()\r\n          if (this.avgEchart.daySend) this.avgEchart.daySend.resize()\r\n          if (this.avgEchart.dayGet) this.avgEchart.dayGet.resize()\r\n          if (this.avgEchart.revenue) this.avgEchart.revenue.resize()\r\n          if (this.avgEchart.carRate) this.avgEchart.carRate.resize()\r\n\r\n          // this.mapEchart.onlineTotal.resize()\r\n          this.mapEchart.szlTotal.resize()\r\n          if (this.hangyeEchart.jingyingqingkuang) this.hangyeEchart.jingyingqingkuang.resize()\r\n          if (this.hangyeEchart.jiedanshichang) this.hangyeEchart.jiedanshichang.resize()\r\n          if (this.hangyeEchart.huoyuecheliang) this.hangyeEchart.huoyuecheliang.resize()\r\n        }, 100)\r\n      }\r\n    })\r\n  },\r\n  methods: {\r\n    // 预警数字接口\r\n    getEarlyWarningNum() {\r\n      axios.get('map/getEarlyWarningNum', { baseURL: this.baseURL }).then(res => {\r\n        this.warning = res.data\r\n      })\r\n    },\r\n    handleTableHeadClick(type) {\r\n      this.leftTopData.activeName = type\r\n      if (type == 'paidan') this.formatterLeftTopEchart('CREATE_NUM')\r\n      else if (type == 'wandan') this.formatterLeftTopEchart('DEP_NUM')\r\n      else if (type == 'huoyuecheliang') this.formatterLeftTopEchart('ACTIVE_NUM')\r\n    },\r\n    formatterLeftTopEchart(type) {\r\n      let all = 0,\r\n        dd = 0,\r\n        sy = 0,\r\n        xd = 0,\r\n        dc = 0,\r\n        cc = 0,\r\n        qt = 0,\r\n        sz = 0,\r\n        ws = 0,\r\n        hh = 0,\r\n        yg = 0\r\n      let qtdata = ['{title|{b}}{abg|}', '  {valueHead|平台}{rateHead|数量(占比)}', '{hr|}']\r\n      let sortData = _.sortBy(this.leftTopData.data, type)\r\n      for (let i = 0; i < sortData.length; i++) {\r\n        if (i < 8) {\r\n          qt += sortData[i][type]\r\n          qtdata.push('{value|' + sortData[i].ABB_NAME + '}{rate|' + sortData[i][type] + '}')\r\n        }\r\n        if (sortData[i].ABB_NAME == '') all = sortData[i][type]\r\n        else if (sortData[i].ABB_NAME == '滴滴出行') dd = sortData[i][type]\r\n      }\r\n      let zhanbi = ((qt / (all - dd)) * 100).toFixed(2)\r\n      let angle = 0\r\n      if (zhanbi < 10 && zhanbi > 0) angle = 0\r\n      else if (zhanbi > 10 && zhanbi < 20) angle = 10\r\n      else if (zhanbi > 20) angle = 75\r\n      this.leftTopData.all = all\r\n      this.leftTopData.dd = dd\r\n      this.rightEchart.left = echarts.init(document.getElementById('rightEchart'))\r\n      this.rightEchart.left.setOption({\r\n        tooltip: {\r\n          trigger: 'axis',\r\n          axisPointer: {\r\n            // 坐标轴指示器，坐标轴触发有效\r\n            type: 'shadow' // 默认为直线，可选为：'line' | 'shadow'\r\n          },\r\n          formatter: function(params, ticket, callback) {\r\n            console.info(params)\r\n            let res = ''\r\n            for (var i = 0, l = params.length; i < l; i++) {\r\n              if (params[i].seriesName != '全部') res += params[i].seriesName + ' : ' + params[i].value + '%'\r\n            }\r\n            return res\r\n          }\r\n        },\r\n        color: ['#e49c23', '#ffffff'],\r\n        legend: {\r\n          data: ['全部', '滴滴出行'],\r\n          show: false\r\n        },\r\n        grid: {\r\n          left: '1%',\r\n          right: '30%',\r\n          bottom: '3%',\r\n          top: '10%',\r\n          containLabel: true\r\n        },\r\n        yAxis: {\r\n          type: 'value',\r\n          show: false\r\n        },\r\n        xAxis: {\r\n          type: 'category',\r\n          data: ['滴滴出行']\r\n        },\r\n        series: [\r\n          {\r\n            name: '滴滴出行',\r\n            type: 'bar',\r\n            stack: '总量',\r\n            label: {\r\n              show: true,\r\n              formatter: '{c} %',\r\n              position: 'insideRight'\r\n            },\r\n            data: [((dd / all) * 100).toFixed(2)]\r\n          },\r\n          {\r\n            name: '全部',\r\n            type: 'bar',\r\n            stack: '总量',\r\n            barWidth: '25%',\r\n            label: {\r\n              show: false\r\n            },\r\n            itemStyle: {\r\n              normal: {\r\n                borderLefWidth: '1', //边框宽度\r\n                borderColor: '#ccc' //边框颜色\r\n              }\r\n            },\r\n\r\n            data: [(((all - dd) / all) * 100).toFixed(2)]\r\n          }\r\n        ]\r\n      })\r\n      this.rightEchart.right = echarts.init(document.getElementById('rightRightEchart'))\r\n\r\n      let rightRight = {\r\n        tooltip: {\r\n          trigger: 'item',\r\n          formatter: '{b} : {c} ({d}%)'\r\n        },\r\n        legend: {\r\n          // orient: 'vertical',\r\n          // top: 'middle',\r\n          bottom: 'bottom',\r\n\r\n          data: ['首汽约车', '享道出行', '多彩出行', '曹操出行', '其他', '神州专车', '万顺叫车', '恒好打车', '阳光出行']\r\n        },\r\n        series: [\r\n          {\r\n            type: 'pie',\r\n            radius: '45%',\r\n            startAngle: angle,\r\n            center: ['38%', '51%'],\r\n            selectedMode: 'single',\r\n            labelLine: {\r\n              show: true,\r\n              length: 5,\r\n              length2: 30\r\n            },\r\n            data: [\r\n              {\r\n                value: qt,\r\n                selected: true,\r\n                name: '其他',\r\n                labelLine: {\r\n                  show: true,\r\n                  length: 20,\r\n                  length2: 70,\r\n                  color: 'orange',\r\n                  lineStyle: {\r\n                    color: 'orange'\r\n                  }\r\n                },\r\n                label: {\r\n                  formatter: qtdata.join('\\n'),\r\n                  backgroundColor: '#eee',\r\n                  borderColor: '#777',\r\n                  borderRadius: 4,\r\n\r\n                  rich: {\r\n                    title: {\r\n                      color: '#eee',\r\n                      align: 'center'\r\n                    },\r\n                    abg: {\r\n                      backgroundColor: '#333',\r\n                      width: '100%',\r\n                      align: 'right',\r\n                      height: 25,\r\n                      borderRadius: [4, 4, 0, 0]\r\n                    },\r\n                    weatherHead: {\r\n                      color: '#333',\r\n                      height: 24,\r\n                      align: 'left'\r\n                    },\r\n                    hr: {\r\n                      borderColor: '#777',\r\n                      width: '100%',\r\n                      borderWidth: 0.5,\r\n                      height: 0\r\n                    },\r\n                    value: {\r\n                      width: 30,\r\n                      height: 30,\r\n                      padding: [0, 20, 0, 15],\r\n                      align: 'center'\r\n                    },\r\n                    valueHead: {\r\n                      color: '#333',\r\n                      height: 24,\r\n                      width: 40,\r\n                      fontSize: 15,\r\n                      padding: [0, 20, 0, 5],\r\n                      align: 'center'\r\n                    },\r\n                    rate: {\r\n                      width: 50,\r\n                      height: 30,\r\n                      align: 'top',\r\n                      padding: [0, 10, 0, 0]\r\n                    },\r\n                    rateHead: {\r\n                      color: '#333',\r\n                      width: 50,\r\n                      fontSize: 15,\r\n                      align: 'center',\r\n                      padding: [0, 10, 0, 0]\r\n                    }\r\n                  }\r\n                }\r\n              }\r\n            ],\r\n            emphasis: {\r\n              itemStyle: {\r\n                shadowBlur: 10,\r\n                shadowOffsetX: 0,\r\n                shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n              }\r\n            }\r\n          }\r\n        ]\r\n      }\r\n      for (let i = 0; i < sortData.length; i++) {\r\n        if (i > 7 && sortData[i].ABB_NAME != '滴滴出行' && sortData[i].ABB_NAME != '') {\r\n          rightRight.series[0].data.push({\r\n            value: sortData[i][type],\r\n            name: sortData[i].ABB_NAME\r\n          })\r\n        }\r\n      }\r\n\r\n      setTimeout(() => {\r\n        this.rightEchart.right.setOption(rightRight, true)\r\n        // this.rightEchart.right.resize()\r\n      }, 500)\r\n    },\r\n    // 营运总览 跳转 日常监测预警\r\n    handleYj(path) {\r\n      this.$emit('jump-event', path)\r\n    },\r\n    echartClick() {\r\n      console.log('echart')\r\n    },\r\n    handleMouseOver(type) {\r\n      for (let i = 0; i < this.dayList.length; i++) {\r\n        if (type == this.dayList[i].AREA_NAME_PY) {\r\n          this.tck.hycl = this.dayList[i].ACTIVE_NUM\r\n          this.tck.ddxq = this.dayList[i].CREATE_NUM\r\n          this.tck.ddcj = this.dayList[i].DEP_NUM\r\n          this.tck.areaName = this.dayList[i].AREA_NAME\r\n        }\r\n      }\r\n      let 