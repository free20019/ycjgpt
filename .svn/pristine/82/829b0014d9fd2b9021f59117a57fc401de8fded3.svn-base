   duration.push((res.data[i].DUR_AVE / 3600).toFixed(2))\r\n          }\r\n          ///111111111\r\n          if (this.avgEchart.daySend) this.avgEchart.daySend.clear()\r\n          this.avgEchart.daySend = echarts.init(document.getElementById('daySend'))\r\n          this.avgEchart.daySend.setOption({\r\n            color: ['#3398DB'],\r\n            tooltip: {\r\n              trigger: 'axis',\r\n              axisPointer: {\r\n                type: 'shadow'\r\n              }\r\n            },\r\n            legend: {\r\n              data: ['日均订单'],\r\n              right: '1%'\r\n            },\r\n            grid: {\r\n              left: '3%',\r\n              right: '4%',\r\n              bottom: '3%',\r\n              top: '15%',\r\n              containLabel: true\r\n            },\r\n            xAxis: [\r\n              {\r\n                type: 'category',\r\n                data: company,\r\n                axisTick: {\r\n                  alignWithLabel: true\r\n                }\r\n              }\r\n            ],\r\n            yAxis: [\r\n              {\r\n                type: 'value'\r\n              }\r\n            ],\r\n            series: [\r\n              {\r\n                name: '日均订单',\r\n                type: 'bar',\r\n                barWidth: '60%',\r\n                data: order\r\n              }\r\n            ]\r\n          })\r\n          //222222\r\n          if (this.avgEchart.dayGet) this.avgEchart.dayGet.clear()\r\n          this.avgEchart.dayGet = echarts.init(document.getElementById('dayGet'))\r\n          this.avgEchart.dayGet.setOption({\r\n            color: ['#3398DB'],\r\n            legend: {\r\n              data: ['日均收益'],\r\n              right: '1%'\r\n            },\r\n            tooltip: {\r\n              trigger: 'axis',\r\n              axisPointer: {\r\n                type: 'shadow'\r\n              }\r\n            },\r\n            grid: {\r\n              left: '3%',\r\n              right: '4%',\